{
  "name": "benching_machine",
  "version": "1.0.0",
  "homepage": "./",
  "description": "A benchmarking tool comparing JavaScript, Rust, and WebAssembly performance",
  "main": "backend/cli.js",
  "scripts": {
    "start": "node backend/cli.js",
    "bench": "node backend/cli.js",
    "serve": "node backend/server.js",
    "web": "react-scripts start",
    "build": "react-scripts build",
    "build:physics": "cd backend/experiments/physics && ./build-asc.sh && ./build-wasm-opt.sh && ./build-wasmedge-aot.sh",
    "build:omp": "cd backend/experiments/swarm && ./build.sh",
    "build:all-wasm": "npm run build:physics && npm run build:omp && cd backend/benchmarks/rust && ./build.sh && echo 'Cheerp: Run backend/benchmarks/cheerp/build.sh manually'",
    "build:rust": "cd backend/benchmarks/rust && ./build.sh",
    "build:cheerp": "cd backend/benchmarks/cheerp && ./build.sh",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "benchmark",
    "webassembly",
    "rust",
    "javascript",
    "performance"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "benchmark": "^2.1.4",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "assemblyscript": "^0.28.9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
